{
  "productName": "Tabula Blockchain",
  "version": "0.1.0",
  "identifier": "com.tabula.blockchain",
  "build": {
    "frontendDist": "../ui"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "iOS": {
      "minimumSystemVersion": "13.0"
    },
    "android": {
      "minSdkVersion": 24
    }
  },
  "app": {
    "windows": [
      {
        "title": "Tabula Blockchain",
        "width": 1200,
        "height": 800,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  }
}

